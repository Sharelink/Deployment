{
  "webroot": "../../../../../wwwroot",
  "version": "1.0.0-*",
  "dependencies": {
    "AuthenticationServer": "1.0.0"
  },
  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel"
  },
  "frameworks": {
    "dnx451": {}
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
